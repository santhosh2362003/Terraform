output "abs_value" {
  value = abs(-10)                  # Output: 10
}

output "ceil_value" {
  value = ceil(2.3)                 # Output: 3
}

output "floor_value" {
  value = floor(2.9)                # Output: 2
}

output "max_value" {
  value = max(1, 2, 5, 3)           # Output: 5
}

output "min_value" {
  value = min(1, 2, 5, 3)           # Output: 1
}